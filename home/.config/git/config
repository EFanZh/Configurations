[diff]
wordRegex = [A-Z]+|[a-z]+|[0-9]+|\\S

[fetch]
prune = yes
pruneTags = yes

[gui]
fontui = -family sans-serif -size 9
fontdiff = -family monospace -size 9
gcwarning = false

[guitool "Open File"]
cmd = xdg-open \"$FILENAME\"
needsfile = yes
noconsole = yes

[guitool "Open with Meld"]
cmd = meld \"$FILENAME\"
needsfile = yes
noconsole = yes

[guitool "Remove File"]
cmd = rm \"$FILENAME\"
needsfile = yes
noconsole = yes

[submodule]
recurse = yes

